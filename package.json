    "requirejs": "^2.2.0",
    "utf8": "^2.1.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^3.4.0",
    "@types/lodash": "^4.14.132",
    "@types/mitm": "^1.3.2",
    "@types/node": "^12.7.3",
    "@types/redis": "^0.12.36",
    "@types/utf8": "^2.1.5",
    "@types/uuid": "^3.4.4",
    "jasmine": "^3.4.0",
    "mitm": "^1.7.0",
    "typescript": "^3.6.2"
  }
