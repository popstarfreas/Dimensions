    "winston": "^3.12.0",
    "@rescript/std": "^11.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jasmine": "^3.4.0",
    "@types/mitm": "^1.3.2",
    "@types/node": "^16.11.6",
